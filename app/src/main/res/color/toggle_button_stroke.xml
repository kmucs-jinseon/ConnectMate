<?xml version="1.0" encoding="utf-8"?>
<!-- Light mode: Toggle button stroke colors -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Checked state: No stroke needed when filled -->
    <item android:color="@color/primary_blue" android:state_checked="true" />
    <!-- Unchecked state: Blue stroke -->
    <item android:color="@color/primary_blue" />
</selector>
